{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "outDir": "dist",
    "types": ["node", "aws-lambda", "jest"]
  },
  "include": [
    "**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "cdk.out"
  ]
}
